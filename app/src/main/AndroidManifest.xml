<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jesse843.taboo_android_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".GameSettingsActivity" />
        <activity android:name=".HowToPlayActivity" />
        <activity android:name=".StoreActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".PauseActivity" />
        <activity android:name=".EndOfRoundActivity" />
        <activity android:name=".GameFinishActivity"></activity>
    </application>

</manifest>